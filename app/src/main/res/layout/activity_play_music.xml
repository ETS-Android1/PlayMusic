<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/musicback"
    tools:context="Activity.PlayMusicActivity">


    <ImageView
        android:id="@+id/ivBack"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="362dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

    <ImageView
        android:id="@+id/ivShare"
        android:layout_width="34dp"
        android:layout_height="35dp"
        android:layout_marginStart="298dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivBack"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_share_24" />

    <ImageView
        android:id="@+id/ivAddToPlayList5"
        android:layout_width="35dp"
        android:layout_height="33dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="308dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivFavorite"
        app:layout_constraintTop_toBottomOf="@+id/ivImagLogo"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/ic_baseline_playlist_add_24" />

    <ImageView
        android:id="@+id/ivAttributes"
        android:layout_width="34dp"
        android:layout_height="33dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toStartOf="@+id/ivTimer"
        app:layout_constraintStart_toEndOf="@+id/ivAddToPlayList5"
        app:layout_constraintTop_toBottomOf="@+id/ivImagLogo"
        app:srcCompat="@drawable/ic_baseline_insert_chart_outlined_24" />

    <ImageView
        android:id="@+id/ivFavorite"
        android:layout_width="31dp"
        android:layout_height="33dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="352dp"
        android:layout_marginEnd="64dp"
        app:layout_constraintEnd_toStartOf="@+id/ivAddToPlayList5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivBack"
        app:srcCompat="@drawable/ic_baseline_favorite_border_24" />

    <ImageView
        android:id="@+id/ivTimer"
        android:layout_width="33dp"
        android:layout_height="31dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="79dp"
        android:layout_marginBottom="312dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivAttributes"
        app:layout_constraintTop_toBottomOf="@+id/ivImagLogo"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_lock_idle_alarm" />

    <TextView
        android:id="@+id/trackName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="169dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="184dp"
        android:text="Track Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivAttributes" />

    <ImageView
        android:id="@+id/ivImagLogo"
        android:layout_width="229dp"
        android:layout_height="229dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="91dp"
        android:background="@drawable/ic_baseline_circle_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_music_note_24" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="286dp"
        android:layout_height="7dp"
        android:layout_margin="60dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivPlayList" />

    <TextView
        android:id="@+id/durationStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="68dp"
        android:text="00.20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/seekBar"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/durationEnd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="68dp"
        android:text="03.40"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/seekBar" />

    <ImageView
        android:id="@+id/PauseVideo"
        android:layout_width="39dp"
        android:layout_height="36dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="59dp"
        android:layout_marginBottom="75dp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar"
        app:layout_constraintStart_toEndOf="@+id/ivPrevious"
        app:layout_constraintTop_toBottomOf="@+id/trackName"
        app:srcCompat="@drawable/ic_baseline_pause_24" />

    <ImageView
        android:id="@+id/ivPrevious"
        android:layout_width="39dp"
        android:layout_height="34dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="78dp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar"
        app:layout_constraintStart_toEndOf="@+id/ivShuffle"
        app:layout_constraintTop_toBottomOf="@+id/ivAddToPlayList5"
        app:srcCompat="@drawable/ic_baseline_skip_previous_24" />

    <ImageView
        android:id="@+id/ivNext"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="229dp"
        android:layout_marginBottom="78dp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar"
        app:layout_constraintStart_toEndOf="@+id/PauseVideo"
        app:layout_constraintTop_toBottomOf="@+id/ivImagLogo"
        app:srcCompat="@drawable/ic_baseline_skip_next_24" />

    <ImageView
        android:id="@+id/ivPlayList"
        android:layout_width="35dp"
        android:layout_height="37dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/durationEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivNext"
        app:srcCompat="@drawable/ic_baseline_playlist_play_24" />

    <ImageView
        android:id="@+id/ivShuffle"
        android:layout_width="32dp"
        android:layout_height="36dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="69dp"
        app:layout_constraintBottom_toTopOf="@+id/durationStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivFavorite"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_baseline_shuffle_24" />

    <com.gauravk.audiovisualizer.visualizer.CircleLineVisualizer
        android:id="@+id/blast"
        android:layout_width="380dp"
        android:layout_height="380dp"
        android:layout_marginTop="97dp"
        android:layout_marginBottom="79dp"
        app:avColor="@color/black"
        app:avDensity=".4"
        app:avSpeed="normal"
        app:avType="fill"
        app:layout_constraintBottom_toTopOf="@+id/ivAddToPlayList5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.515" />


</androidx.constraintlayout.widget.ConstraintLayout>